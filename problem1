import sys


def main(file_name):
    with open(file_name) as file:
        line = file.readline()
        counter = 0
        number_prev = line
        while line:
            line = file.readline()
            number_curr = line
            if number_curr == '':
                break
            if int(number_curr) > int(number_prev):
                counter = counter + 1
            number_prev = number_curr

    return counter


if __name__ == "__main__":
    number_of_increases = main(sys.argv[1])
    print(str(number_of_increases))
